alias ll ls -lh $*
alias ls ls --color=tty $*
alias lsa ls -lah $*
alias md mkdir -p $*
alias gaa git add . $*
alias gii git init $*
alias gst git status $*
alias gsh git stash $*
alias gd git diff $*
